---
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - all
    - posts
layout: "layouts/base.njk"
permalink: "/tag/{{ tag | slugify }}/"
eleventyExcludeFromCollections: "true"
title: "Tag archive."
description: "Tag archive."
coverImage: "culturednyc-og-image.png"
---

<h3>Tag archive: {{ tag }}</h3>
<br>
<div class="post-list">
<ul>

{% set taglist = collections[ tag ] %}

{% for post in taglist | reverse %}

  <h1><li><a href="{{ post.url }}">{{ post.data.title }}</a></li></h1>

  {% include 'layouts/byline.njk' %}

  <br>

  {{ post.data.description }}

  <p></p>

{% endfor %}

</ul>
</div>